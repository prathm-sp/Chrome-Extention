{
    "name": "Extention2",
    "version": "1.0",
    "description": "display something",
    "manifest_version": 2,
    "chrome_url_overrides": {
        "newtab": "newTab.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}